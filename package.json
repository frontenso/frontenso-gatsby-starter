{
  "name": "frontenso-gatsby-starter",
  "private": true,
  "description": "A starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "einomi",
  "dependencies": {
    "@frontenso/eslint-config-javascript": "^0.1.3",
    "@frontenso/eslint-config-react": "^0.1.0",
    "@linaria/babel-preset": "^3.0.0-beta.1",
    "@linaria/core": "^3.0.0-beta.1",
    "@linaria/react": "^3.0.0-beta.1",
    "@linaria/shaker": "^3.0.0-beta.1",
    "@linaria/stylelint": "^3.0.0-beta.1",
    "@linaria/webpack-loader": "^3.0.0-beta.1",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^0.12.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "gatsby": "^2.32.6",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-image": "^0.7.2",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-postcss": "^3.7.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.14.2",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "postcss": "^8.2.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "stylelint": "^13.11.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "husky": "4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop",
    "build": "cross-env NODE_ENV=production GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true npm run validate:ci && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "pretty-quick --staged",
    "lint:css": "stylelint \"src/**/*.{jsx,js}\"",
    "lint:js": "eslint --max-warnings 0 \"src/**/*.{jsx,js}\"",
    "validate": "npm-run-all --parallel format lint:css lint:js",
    "validate:ci": "npm-run-all --parallel lint:css lint:js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frontenso/frontenso-gatsby-starter"
  }
}
